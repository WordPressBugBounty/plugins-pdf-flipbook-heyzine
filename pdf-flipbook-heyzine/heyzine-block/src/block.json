{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "cl-heyzine-block/heyzine-block",
	"version": "0.2.0",
	"title": "Heyzine",
	"category": "embed",
	"icon": "heyzine",
	"description": "Embed a Heyzine Flipbook",
	"supports": {
		"html": true
	},
	"textdomain": "pdf-flipbook-heyzine",
	"attributes": {
		"message": {
			"type": "string",
			"default": "Heyzine embed block"
		},
		"heyzines": {
			"type": "object"
		},
		"heyzineSelected": {
			"type": "string"
		},
		"heyzineBorder": {
			"type": "string"
		},
		"heyzineFold": {
			"type": "string"
		},
		"heyzineLink": {
			"type": "string"
		},
		"heyzineLinkPage": {
			"type": "string",
			"default": "https://heyzine.com/"
		},
		"heyzinePages": {
			"type": "integer",
			"default": 0
		},
		"heyzineEmbedPage": {
			"type": "integer",
			"default": 0
		},
		"customizeDisabled": {
			"type": "boolean",
			"default": true
		},
		"showBorder": {
			"type": "boolean",
			"default": false
		},
		"showFold": {
			"type": "boolean",
			"default": false
		},
		"responsiveWidth": {
			"type": "boolean",
			"default": true
		},
		"pagesEnabled": {
			"type": "boolean",
			"default": false
		},
		"flibookWidth": {
			"type": "integer",
			"default": 800
		},
		"flibookHeight": {
			"type": "integer",
			"default": 500
		}
	},

	"keywords": [
		"heyzine",
		"embed",
		"magazine",
		"catalog",
		"report",
		"publication",
		"pdf",
		"flipbook",
		"flip book"
	],
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
